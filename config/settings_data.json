      ".footer-block__heading {font-size: 25px;}",
      ".card-information .price {margin: 0 !important;}",
      ".button,.shopify-challenge__button,.customer button,button.shopify-payment-button__button--unbranded {min-width: calc(12rem + var(--buttons-border-width) * 2); min-height: calc(4.5rem + var(--buttons-border-width) * 2); border-radius: 30px;}",
